   "source": [
    "# Broadcasting \n",
    "# Rules - Array has same dimension or 1 in the shape eg: ((1,3) & (3,1)), (2,3) & (2,1)\n",
    "import numpy as np\n",
    "arr = np.array([1,2,3])\n",
    "print(arr)\n",
    "print(arr.shape)\n",
    "arr1 = np.array([[1], [2], [3]])\n",
    "print(arr1)\n",
    "print(arr1.shape)\n",
    "print(arr + arr1)\n"
   ]
  }
